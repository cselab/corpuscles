include(`he.m4')dnl
include(`arg.m4')dnl
h_define(`map', `h_map_sep(`$1', `, \
', `$2')')dnl
h_define(`F', `"$3" binary filetype=png center=($1,$2) dx=dx dy=dy with rgbalpha notitle')dnl

set term pngcairo size 800, 600
set output "q.png"

#set size sq

set xlabel '{/Symbol D}a'
set ylabel 'v'

set lmargin -5
set rmargin -5
set bmargin 4

xl=0.95
xh=1.64

yl=0.475
yh=0.725

lx = xh - xl
ly = yh - yl

dx = 0.00045
dy = dx*ly/lx

plot [xl:xh][yl:yh] \
map(`F', `Arg')
