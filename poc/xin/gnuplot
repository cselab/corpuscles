include(`he.m4')dnl
include(`arg.m4')dnl
h_define(`map', `h_map_sep(`$1', `, \
', `$2')')dnl
h_define(`F', `"$3" binary filetype=png center=($1,$2) dx=dx dy=dy with rgbalpha notitle')dnl

set output "q.png"

nx = 800
ny = 600

xl=0.95
xh=1.64

yl=0.475
yh=0.725

lx = xh - xl
ly = yh - yl

dx = 0.00045
dy = dx*ny/nx

set term pngcairo size nx, ny

set size sq
plot [xl:xh][yl:yh] \
map(`F', `Arg')
