@Article{Svetina1989,
  author   = {Svetina, S. and \v{Z}ek\v{s}, B.},
  title    = {Membrane bending energy and shape determination of phospholipid vesicles and red blood cells},
  journal  = {European Biophysics Journal},
  year     = {1989},
  volume   = {17},
  number   = {2},
  pages    = {101--111},
  month    = {Jun},
  abstract = {A procedure is developed to calculate red blood cell and phospholipid vesicle shapes within the bilayer couple model of the membrane. The membrane is assumed to consist of two laterally incompressible leaflets which are in close contact but unconnected. Shapes are determined by minimizing the membrane bending energy at a given volume of a cell (V), given average membrane area (A) and given difference of the areas of two leaflets ($\Delta$A). Different classes of shapes exist in parts of the v/$\Delta$a phase diagram, where v and $\Delta$a are the volume and the leaflet area difference relative to the sphere with area A. The limiting shapes are composed of sections of spheres with only two values allowed for their radii. Two low energy axisymmetrical classes, which include discocyte and stomatocyte shapes are studied and their phase diagrams are analyzed. For v=0.6, the discocyte is the lowest energy shape, which transforms by decreasing $\Delta$a continuously into a stomatocyte. The spontaneous membrane curvature (C0) and compressibility of membrane leaflest can be incorporated into the model.},
  day      = {01},
  doi      = {10.1007/BF00257107},
  groups   = {biomembrane, vesicle_bilayer_couple_model, vesicle_local_curvature, membrane_red_blood_cell, membrane_vesicle, membrane},
  issn     = {1432-1017},
  url      = {https://doi.org/10.1007/BF00257107},
}

