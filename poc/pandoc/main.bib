@Article{Barns2017,
  author   = {Barns, Sarah and Balanant, Marie Anne and Sauret, Emilie and Flower, Robert and Saha, Suvash and Gu, YuanTong},
  title    = {Investigation of red blood cell mechanical properties using AFM indentation and coarse-grained particle method},
  journal  = {BioMedical Engineering OnLine},
