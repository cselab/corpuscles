@Article{Hoekstra2014,
  author   = {Hoekstra, Alfons and Chopard, Bastien and Coveney, Peter},
  title    = {Multiscale modelling and simulation: a position paper},
  journal  = {Phil. Trans. R. Soc. A},
  year     = {2014},
  volume   = {372},
  number   = {2021},
  abstract = {We argue that, despite the fact that the field of multiscale modelling
	and simulation has enjoyed significant success within the past decade,
	it still holds many open questions that are deemed important but
	so far have barely been explored. We believe that this is at least
	in part due to the fact that the field has been mainly developed
	within disciplinary silos. The principal topics that in our view
	would benefit from a targeted multidisciplinary research effort are
	related to reaching consensus as to what exactly one means by ‘multiscale
	modelling’, formulating a generic theory or calculus of multiscale
	modelling, applying such concepts to the urgent question of validation
	and verification of multiscale models, and the issue of numerical
	error propagation in multiscale models. Moreover, we believe that
	this would, in principle, also lay the foundation for more efficient,
	well-defined and usable multiscale computing environments. We believe
	that multidisciplinary research to fill in the gaps is timely, highly
	relevant, and with substantial potential impact on many scientific
	disciplines.},
  doi      = {10.1098/rsta.2013.0377},
  eprint   = {http://rsta.royalsocietypublishing.org/content/372/2021/20130377.full.pdf+html},
  file     = {:2014_Hoekstra_Chopard_Coveney.pdf:PDF},
  groups   = {multiscale coupling and modeling, reviews, books and perspectives, reviews, books and perspectives, special issues},
  url      = {http://rsta.royalsocietypublishing.org/content/372/2021/20130377.abstract},
}

