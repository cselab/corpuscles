@Article{Wang2015b,
  author   = {Wang, Jianchun and Li, Qianxiao and E, Weinan},
  title    = {Study of the instability of the Poiseuille flow using a thermodynamic formalism},
  journal  = {Proceedings of the National Academy of Sciences},
  year     = {2015},
  volume   = {112},
  number   = {31},
  pages    = {9518-9523},
  abstract = {The stability of the plane Poiseuille flow is analyzed using a thermodynamic
	formalism by considering the deterministic Navier–Stokes equation
	with Gaussian random initial data. A unique critical Reynolds number,
	Rec≈2,332, at which the probability of observing puffs in the solution
	changes from 0 to 1, is numerically demonstrated to exist in the
	thermodynamic limit and is found to be independent of the noise amplitude.
	Using the puff density as the macrostate variable, the free energy
	of such a system is computed and analyzed. The puff density approaches
	zero as the critical Reynolds number is approached from above, signaling
	a continuous transition despite the fact that the bifurcation is
	subcritical for a finite-sized system. An action function is found
	for the probability of observing puffs in a small subregion of the
	flow, and this action function depends only on the Reynolds number.
	The strategy used here should be applicable to a wide range of other
	problems exhibiting subcritical instabilities.},
  doi      = {10.1073/pnas.1501288112},
  eprint   = {http://www.pnas.org/content/112/31/9518.full.pdf},
  file     = {:2016/2016b_Wang_Li_E.pdf:PDF},
  groups   = {fluid_mechanics},
  url      = {http://www.pnas.org/content/112/31/9518.abstract},
}

