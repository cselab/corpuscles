  url       = {http://dx.doi.org/10.1039/B616490K},
}

@Article{Bocquet1996,
  author   = {L. Bocquet and J.-L. Barrat},
  title    = {Hydrodynamic properties of confined fluids},
  journal  = {J. Phys.: Condens. Matter},
  year     = {1996},
  volume   = {8},
  number   = {47},
  pages    = {9297},
  abstract = {In the present work, we study the dynamical properties of fluids confined
	at molecular scales. First, we show that, for a fluid confined between
	two parallel solid walls, the phenomenological hydrodynamic description
	of bulk fluids holds down to nanoscopic scales, once appropriate
	boundary conditions at the solid walls are applied. Then, we investigate
	the influence of confinement on the self-diffusion coefficient (in
	the direction parallel to the walls) in a fluid slab. We have computed
	the effect of confinement on the mode-coupling contribution to the
	diffusion coefficient. These finite-size corrections are shown to
	reduce the diffusion constant by an amount ##IMG## [http://ej.iop.org/images/0953-8984/8/47/019/img5.gif]
	, where h is the thickness of the fluid slab and ##IMG## [http://ej.iop.org/images/0953-8984/8/47/019/img6.gif]
	the atomic size. This behaviour can be interpreted in terms of the
	suppression of long-wavelength modes in the backflow effect, due
	to confinement.},
  file     = {:1995_1999/1996_Bocquet_Barrat.pdf:PDF},
  groups   = {solid-liquid interface, interface boundary conditions, fluid_mechanics},
  url      = {http://stacks.iop.org/0953-8984/8/i=47/a=019},
}

@Article{Bocquet1995,
  author   = {L. Bocquet and J.-L. Barrat},
  title    = {Diffusive Motion in Confined Fluids: Mode-Coupling Results and Molecular-Dynamics Calculations},
  journal  = {EPL (Europhysics Letters)},
  year     = {1995},
  volume   = {31},
  number   = {8},
  pages    = {455},
  abstract = {We present a mode-coupling calculation of the self-diffusion coefficient
	in a liquid confined between two parallel solid walls. The effect
	of confinement is to lower the diffusion constant in the direction
	parallel to the walls, compared to the bulk case. The relative decrease
	behaves as (σ/ h ) log ( h /σ), where h is the thickness of the fluid
	slab, and σ the atomic size. This behaviour is interpreted in terms
	of the suppression of long-wavelength modes due to confinement. It
	is also explained by a simple hydrodynamic argument. Molecular-dynamics
	simulations confirm the theoretically predicted behaviour, even at
	rather small values of h .},
  file     = {:1995_1999/1995_Bocquet_Barrat.pdf:PDF},
  groups   = {mode coupling theory, and CG-, numeric_md},
  url      = {http://stacks.iop.org/0295-5075/31/i=8/a=006},
}

