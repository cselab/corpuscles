@Article{Markesteijn2014,
  author   = {Markesteijn, Anton and Karabasov, Sergey and Scukins, Arturs and Nerukh, Dmitry and Glotov, Vyacheslav and Goloviznin, Vasily},
  title    = {Concurrent multiscale modelling of atomistic and hydrodynamic processes in liquids},
  journal  = {Phil. Trans. R. Soc. A},
  year     = {2014},
  volume   = {372},
  number   = {2021},
  abstract = {Fluctuations of liquids at the scales where the hydrodynamic and atomistic
	descriptions overlap are considered. The importance of these fluctuations
	for atomistic motions is discussed and examples of their accurate
	modelling with a multi-space–time-scale fluctuating hydrodynamics
	scheme are provided. To resolve microscopic details of liquid systems,
	including biomolecular solutions, together with macroscopic fluctuations
	in space–time, a novel hybrid atomistic–fluctuating hydrodynamics
	approach is introduced. For a smooth transition between the atomistic
	and continuum representations, an analogy with two-phase hydrodynamics
	is used that leads to a strict preservation of macroscopic mass and
	momentum conservation laws. Examples of numerical implementation
	of the new hybrid approach for the multiscale simulation of liquid
	argon in equilibrium conditions are provided.},
  doi      = {10.1098/rsta.2013.0379},
  eprint   = {http://rsta.royalsocietypublishing.org/content/372/2021/20130379.full.pdf+html},
  file     = {:2014_Markesteijn_Karabasov_Scukins_Nerukh_Glotov_Goloviznin.pdf:PDF},
  groups   = {multiscale coupling and modeling},
  url      = {http://rsta.royalsocietypublishing.org/content/372/2021/20130379.abstract},
}

