@Article{Teschner2016,
  author   = {Teschner, Tom-Robin and K{\"o}n{\"o}zsy, L{\'a}szl{\'o} and Jenkins, Karl W.},
  title    = {Progress in particle-based multiscale and hybrid methods for flow applications},
  journal  = {Microfluidics and Nanofluidics},
  year     = {2016},
  volume   = {20},
  number   = {4},
  pages    = {1--38},
  issn     = {1613-4990},
  abstract = {This work focuses on the review of particle-based multiscale and hybrid
	methods that have surfaced in the field of fluid mechanics over the
	last 20 years. We consider five established particle methods: molecular
	dynamics, direct simulation Monte Carlo, lattice Boltzmann method,
	dissipative particle dynamics and smoothed-particle hydrodynamics.
	A general description is given on each particle method in conjunction
	with multiscale and hybrid applications. An analysis on the length
	scale separation revealed that current multiscale methods only bridge
	across scales which are of the order of \$\$\{{\backslash}mathcal
	\{O\}\}(10^2) \{-\} \{{\backslash}mathcal \{O\}\}(10^3)\$\$ O ( 10
	2 ) - O ( 10 3 ) and that further work on complex geometries and
	parallel code optimisation is needed to increase the separation.
	Similarities between methods are highlighted and combinations discussed.
	Advantages, disadvantages and applications of each particle method
	have been tabulated as a reference.},
  doi      = {10.1007/s10404-016-1729-y},
  file     = {:2016/2016_Teschner_Koenoezsy_Jenkins.pdf:PDF},
  groups   = {reviews, books and perspectives, reviews and books, reviews, books and perspectives, special issues},
  url      = {http://dx.doi.org/10.1007/s10404-016-1729-y},
}

@Article{Theodoropoulos2000,
  author   = {Theodoropoulos, Constantinos and Qian, Yue-Hong and Kevrekidis, Ioannis G.},
  title    = {“Coarse” stability and bifurcation analysis using time-steppers: A reaction-diffusion example},
  journal  = {Proc. Natl. Acad. Sci. USA},
  year     = {2000},
  volume   = {97},
  number   = {18},
  pages    = {9840-9843},
  abstract = {Evolutionary, pattern forming partial differential equations (PDEs)
	are often derived as limiting descriptions of microscopic, kinetic
	theory-based models of molecular processes (e.g., reaction and diffusion).
	The PDE dynamic behavior can be probed through direct simulation
	(time integration) or, more systematically, through stability/bifurcation
	calculations; time-stepper-based approaches, like the Recursive Projection
	Method [Shroff, G. M. & Keller, H. B. (1993) SIAM J. Numer. Anal.
	30, 1099–1120] provide an attractive framework for the latter. We
	demonstrate an adaptation of this approach that allows for a direct,
	effective (“coarse”) bifurcation analysis of microscopic, kinetic-based
	models; this is illustrated through a comparative study of the FitzHugh-Nagumo
	PDE and of a corresponding Lattice–Boltzmann model.},
  doi      = {10.1073/pnas.97.18.9840},
  eprint   = {http://www.pnas.org/content/97/18/9840.full.pdf},
  file     = {:2000/2000_Theodoropoulos_Qian_Kevrekidis.pdf:PDF},
  groups   = {multiscale coupling and modeling, equation free},
}

