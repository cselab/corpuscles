@Article{Kim2015,
  author   = {Kim, C. and Karniadakis, G. E.},
  title    = {Brownian Motion of a {Rayleigh} Particle Confined in a Channel: A Generalized {Langevin} Equation Approach},
  journal  = {J. Stat. Phys.},
  year     = {2015},
  volume   = {158},
  number   = {5},
  pages    = {1100--1125},
  abstract = {We study confined Brownian motion by investigating the memory function
	of a \$\$d\$\$ d -dimensional hypercube ( \$\$d{\backslash}ge 2\$\$
	d ≥ 2 ), which is subject to a harmonic potential and suspended in
	an ideal gas confined by two parallel walls. For elastic walls and
	under the infinite-mass limit, we obtain analytic expressions for
	the force autocorrelation function and the memory function. The transverse-direction
	memory function possesses a nonnegative tail decaying like \$\$t^\{-(d-1)\}\$\$
	t - ( d - 1 ) , from which anomalous diffusion is expected for \$\$d=2\$\$
	d = 2 . For \$\$d=3\$\$ d = 3 , the position-dependent friction coefficient
	becomes larger than the unconfined case and the increment is inversely
	proportional to the square of the distance from the wall. We also
	perform molecular dynamics simulations with thermal walls and/or
	a finite-mass hypercube. We observe faster decay due to the thermal
	wall ( \$\$t^\{-3\}\$\$ t - 3 for \$\$d=2\$\$ d = 2 and \$\$t^\{-5\}\$\$
	t - 5 for \$\$d=3\$\$ d = 3 under the fully thermalizing wall) and
	convergence behaviors of the finite-mass memory function, which are
	different from the unconfined case.},
  doi      = {10.1007/s10955-014-1160-2},
  file     = {:2015/2015_Kim_Karniadakis.pdf:PDF},
  groups   = {Brownian motion},
  issn     = {1572-9613},
  url      = {http://dx.doi.org/10.1007/s10955-014-1160-2},
}

