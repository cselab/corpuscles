@Article{Ganzenmueller2015,
  author   = {G.C. Ganzenmüller and S. Hiermaier and M. May},
  title    = {On the similarity of meshless discretizations of Peridynamics and Smooth-Particle Hydrodynamics},
  journal  = {Computers \& Structures},
  year     = {2015},
  volume   = {150},
  number   = {0},
  pages    = {71 - 78},
  abstract = {Abstract This paper discusses the similarity of meshless discretizations
	of Peridynamics and Smooth-Particle-Hydrodynamics (SPH), if Peridynamics
	is applied to classical material models based on the deformation
	gradient. We show that the discretized equations of both methods
	coincide if nodal integration is used. This equivalence implies that
	Peridynamics reduces to an old meshless method and all instability
	problems of collocation-type particle methods apply. These instabilities
	arise as a consequence of the nodal integration scheme, which causes
	rank-deficiency and leads to spurious zero-energy modes. As a result
	of the demonstrated equivalence to SPH, enhanced implementations
	of Peridynamics should employ more accurate integration schemes.
	},
  doi      = {http://dx.doi.org/10.1016/j.compstruc.2014.12.011},
  file     = {:2015_Ganzenmueller_Hiermaier_May.pdf:PDF},
  groups   = {, peridynamics, numeric_sph},
  issn     = {0045-7949},
  keywords = {Meshless methods},
  url      = {http://www.sciencedirect.com/science/article/pii/S0045794914002971},
}

