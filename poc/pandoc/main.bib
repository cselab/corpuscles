@Article{Lee2003,
  author                     = {Lee, YS and Wetzel, ED and Wagner, NJ},
  title                      = {The ballistic impact characteristics of Kevlar woven fabrics impregnated with a colloidal shear thickening fluid},
  journal                    = {J. Mater. Sci.},
  year                       = {2003},
  volume                     = {38},
  number                     = {13},
  pages                      = {2825-2833},
  abstract                   = {{This study reports the ballistic penetration performance of a composite
	material composed of woven Kevlar(R) fabric impregnated with a colloidal
	shear thickening fluid (silica particles (450 nm) dispersed in ethylene
	glycol). The impregnated Kevlar fabric yields a flexible, yet penetration
