#!/bin/sh

. co.util
prog=co.list
usg () {
cat >&2 <<!
$prog OFF [OFF..] < OFF > OFF
!
exit 2
}

for i
do case "$i" in
       -h) usg ;;
       -*) err "unknown option '$i'" ;;
   esac
done

if test $# -eq 0
then err 'needs argument OFF'
fi

for i
do if ! test -f "$i"
   then err "$i is not a file"
   fi
done


echo LIST
for i
do echo '{'
   cat "$i"
   echo '}'
done
echo '{'
cat
echo '}'
